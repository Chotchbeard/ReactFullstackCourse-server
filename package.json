{
  "name": "react-fullstack-course-server",
  "version": "1.0.0",
  "description": "Backend API server for React Fullstack Udemy course",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "repository": {
    "type": "git",
