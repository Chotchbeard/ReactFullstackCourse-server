    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "husky": "^1.1.2",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0"
  }
